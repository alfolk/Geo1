<odoo>
   <!-- Inherit Form View to Modify it -->
   <record id="view_partner_form" model="ir.ui.view">
       <field name="name">res.partner</field>
       <field name="model">res.partner</field>
       <field name="inherit_id" ref="base.view_partner_form"/>
       <field name="arch" type="xml">

                  <div name="button_box" position="inside">
                        <button class="oe_stat_button o_res_partner_tip_opp" type="object" name="open_medical_form" icon="fa-user-md"
                         context="{'default_partner_id': active_id}">
                            Medical File
                        </button>
                    </div>
           <div name="button_box" position="inside">
                        <button class="oe_stat_button o_res_partner_tip_opp" type="object" name="open_physical_measurements" icon="fa-universal-access"
                         context="{'default_partner_id': active_id}">
                          Measurements
                        </button>
                    </div>
           <div name="button_box" position="inside">
                        <button class="oe_stat_button o_res_partner_tip_opp" type="object" name="open_nutrition" icon="fa-envira"
                         context="{'default_partner_id': active_id}">
                            Nutrition
                        </button>
                    </div>
           <div name="button_box" position="inside">
                        <button class="oe_stat_button o_res_partner_tip_opp" type="object" name="open_achievement_rate" icon="fa-trophy"
                         context="{'default_partner_id': active_id}">
                          Achievement
                        </button>
                    </div>

       </field>
   </record>

</odoo>